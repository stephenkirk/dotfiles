[include]
    path = ~/.gitconfig.local
[column]
	ui = auto
[branch]
	sort = -committerdate
[tag]
	sort = version:refname
[init]
	defaultBranch = main
[diff]
	algorithm = histogram
	colorMoved = plain
	renames = true
[push]
	autoSetupRemote = true
	followTags = true
[rerere]
	enabled = true
	autoupdate = true
